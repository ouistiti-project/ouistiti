user="%USER%";
log-file="%LOGFILE%";
servers= ({
		hostname = "www.ouistiti.net";
		port = 8080;
		keepalivetimeout = 5;
		version="HTTP11";
		vhost = (
		{
			hostname = "myvhost.ouistiti.net";
			auth = {
				type="Basic";
				realm="www.ouistiti.net";
				user="foo";
				passwd="bar";
			};
			document = {
				docroot = "%PWD%/tests/htdocs/vhost";
				allow = ".html,.*htm*,.css,.js,.txt,*";
				deny = ".htaccess,.cgi,*.php";
				options = "dirlisting,range,rest";
			};
			redirect = {
				links = ({
					options = "error";
					destination = "error_404.html";
					origin = 404;
				},{
					options = "generate_204";
					origin = "empty";
				});

			};
		},{
			hostname = "myvhost2.ouistiti.net";
			service = "myvhost2";
			document = {
				docroot = "%PWD%/tests/htdocs/";
                                allow = ".html,.*htm*,.css,.js,.txt,*";
                                deny = ".htaccess,.cgi,*.php";
                                options = "dirlisting,range";
			};
		},"%PWD%/tests/conf/config.d/test20.vhost");
	});


