user="%USER%";
log-file="%LOGFILE%";
servers= ({
		hostname = "www.ouistiti.net";
		port = 8080;
		keepalivetimeout = 5;
		version="HTTP11";
		document = {
			docroot = "htdocs";
			allow = ".html,.*htm*,.css,.js,.txt,*";
			deny = ".htaccess,.php";
			options = "dirlisting,sendfile,range";
			defaultpage = "index.html";
		};
	});


